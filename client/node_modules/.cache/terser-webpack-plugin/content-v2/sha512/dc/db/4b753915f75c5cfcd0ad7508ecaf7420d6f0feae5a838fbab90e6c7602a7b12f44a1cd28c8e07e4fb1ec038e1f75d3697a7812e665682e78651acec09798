 faster, you can change\\n// unregister() to register() below. Note this comes with some pitfalls.\\n// Learn more about service workers: http://bit.ly/CRA-PWA\\nserviceWorker.unregister();\\n\"]}","code":"(window.webpackJsonp=window.webpackJsonp||[]).push([[0],{14:function(e,t,n){},16:function(e,t,n){},18:function(e,t,n){\"use strict\";n.r(t);var a=n(0),r=n.n(a),l=n(7),c=n.n(l),o=(n(14),n(1)),i=n(2),u=n(4),s=n(3),h=n(5),m=(n(16),n(8)),d=function(e){function t(){var e,n;Object(o.a)(this,t);for(var a=arguments.length,r=new Array(a),l=0;l<a;l++)r[l]=arguments[l];return(n=Object(u.a)(this,(e=Object(s.a)(t)).call.apply(e,[this].concat(r)))).state={topic:\"\",startYear:\"\",endYear:\"\"},n.handleInputChange=function(e){var t=e.target.value,a=e.target.name;n.setState(Object(m.a)({},a,t))},n.handleFormSubmit=function(e){e.preventDefault()},n}return Object(h.a)(t,e),Object(i.a)(t,[{key:\"render\",value:function(){return r.a.createElement(\"div\",null,r.a.createElement(\"form\",{className:\"form\"},r.a.createElement(\"input\",{value:this.state.topic,name:\"Topic\",onChange:this.handleInputChange,type:\"text\",placeholder:\"Topic\"}),r.a.createElement(\"input\",{value:this.state.startYear,name:\"Start Year\",onChange:this.handleInputChange,type:\"text\",placeholder:\"Start Year\"}),r.a.createElement(\"input\",{value:this.state.endYear,name:\"End Year\",onChange:this.handleInputChange,type:\"password\",placeholder:\"End Year\"}),r.a.createElement(\"button\",{onClick:this.handleFormSubmit},\"Search\")))}}]),t}(a.Component),p=function(e){return r.a.createElement(\"div\",{id:\"search\"},r.a.createElement(\"h4\",null,\"Search\"),r.a.createElement(d,null))},v=function(){return r.a.createElement(\"div\",{id:\"results\"},r.a.createElement(\"h4\",null,\"Results\"),r.a.createElement(\"div\",null))},E=function(e){return r.a.createElement(\"div\",{id:\"saved\"},r.a.createElement(\"h4\",null,\"Saved Articles\"),r.a.createElement(\"div\",null))},f=function(e){function t(){return Object(o.a